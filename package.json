{
  "name": "neofelis-backend",
  "version": "1.0.0",
  "description": "Neofelis backend server",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "hasura": "hasura --project ./hasura --envfile .env --skip-update-check",
    "metadata:export": "pnpm run hasura metadata export",
    "metadata:apply": "pnpm run hasura metadata apply",
    "code-gen": "graphql-codegen",
    "dbgenerate": "drizzle-kit generate --config=./src/drizzle.config.ts",
    "dbpush:script": "bun ./src/drizzle/migrate.ts",
    "dbpush": "drizzle-kit push --config=./src/drizzle.config.ts"
  },
  "dependencies": {
    "@golevelup/nestjs-graphql-request": "^0.2.5",
    "@golevelup/nestjs-hasura": "^3.0.11",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/swagger": "^11.2.0",
    "@rschedule/core": "^1.5.0",
    "@rschedule/dayjs-date-adapter": "^1.5.0",
    "@rschedule/standard-date-adapter": "^1.5.0",
    "apollo-server-express": "^3.11.1",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "express": "^5.1.0",
    "graphql": "^16.6.0",
    "graphql-request": "^7.2.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.6",
    "joi": "^17.8.3",
    "lodash": "^4.17.21",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
